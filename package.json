{
  "name": "takenote",
  "version": "0.7.2",
  "description": "A web-based notes app for developers.",
  "author": "Tania Rascia",
  "license": "MIT",
  "private": false,
  "main": "src/server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "server": "nodemon --config config/nodemon.config.json",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "prod": "node -r ts-node/register/transpile-only src/server/index.ts",
    "start": "npm run client",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "eslint": "eslint src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taniarascia/takenote"
  },
  "keywords": [
    "notes",
    "notes-app",
    "note-taking",
    "markdown",
    "markdown-editor",
    "redux",
    "react",
    "typescript",
    "react-hooks",
    "react-hooks-redux",
    "github"
  ],
  "homepage": "",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
    "clipboard-polyfill": "^3.0.3",
    "codemirror": "^5.63.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jszip": "^3.7.1",
    "mousetrap": "^1.6.5",
    "mousetrap-global-bind": "^1.1.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.4.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-codemirror2": "^7.2.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet-async": "^1.1.2",
    "react-markdown": "^7.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-split-pane": "^0.1.92",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "unist-util-visit": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/axios": "^0.14.0",
    "@types/codemirror": "5.60.5",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/helmet": "0.0.48",
    "@types/jest": "^27.0.2",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.6",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.33",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.10",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-redux": "^7.1.20",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "clipboardy": "^3.0.0",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "cypress": "^8.7.0",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "faker": "^5.5.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.3.1",
    "jest-extended": "^1.1.0",
    "jest-raw-loader": "^1.0.1",
    "lint-staged": "^11.2.4",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.14",
    "sass-loader": "^12.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  }
}
